<KivyCamera>:
    circle_pos: root.circle_pos
    circle_r: root.circle_r
    # texture: root.update()

    Rings:
        id: ring1
        pos: root.circle_pos
        radius: root.circle_r


<Screen>:
    KivyCamera:
        id: cam


