#:import win kivy.core.window

<Rings>:
    radius: root.radius
    canvas:
        Color:
            rgb: 0.502, 0.2588, 0.9569
        Line:
            width: 3
            circle: self.pos[0], self.pos[1], self.radius
        Color:
            rgb: 0.2588, 0.3725, 0.9569
        Line:
            width: 3
            circle: self.pos[0], self.pos[1], self.radius +10
        Color:
            rgb: 0.2392, 1., 1.
        Line:
            width: 3
            circle: self.pos[0], self.pos[1], self.radius +20

<KivyCamera>:
    circle_pos: root.circle_pos
    circle_r: root.circle_r
    # texture: root.update()






<Screen>:
    KivyCamera:
        id: cam

    Rings:
        id: ring1
        pos: cam.circle_pos
        radius: cam.circle_r


